{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "globals": { },
  "env": {
    "node": true
  },
  "extends": "standard",
  "plugins": [
        "standard",
        "promise"
  ],

  "rules": {
    "no-multiple-empty-lines": "off",
    "padded-blocks": "off",
    "object-property-newline": "off",
    "space-unary-ops": "off",
    "quotes": "off"
  }
}